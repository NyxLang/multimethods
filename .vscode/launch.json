    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Tests",
            "program": "${workspaceRoot}/scripts/run-unit-tests.js",
            "outFiles": [
                "${workspaceRoot}/dist/**/*.js"
            ],
            "env": {
                // "DEBUG": "multimethods"
            },
            "skipFiles": [
                // "<node_internals>/**"
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Benchmarks",
            "program": "${workspaceRoot}/dist/extras/bench/basic-routing.js",
            "outFiles": [
                "${workspaceRoot}/dist/**/*.js"
            ],
            "env": {
                "DEBUG": "multimethods"
            },
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}
{
  "name": "multimethods",
  "version": "0.2.12",
  "description": "multimethods",
  "readmeFilename": "README.md",
  "main": "dist/release/index.js",
  "typings": "dist/release/index.d.ts",
  "scripts": {
    "build": "tsc -p src && tsc -p test && tsc -p extras && npm run build:peg",
    "build:peg": "pegjs -o dist/release/set-theory/predicates/dsl-grammar.js src/set-theory/predicates/dsl-grammar.pegjs",
    "clean": "rimraf dist/",
    "test": "node ./scripts/run-unit-tests",
    "test:debug": "cross-env DEBUG=multimethods node ./scripts/run-unit-tests",
    "bench": "node ./dist/extras/bench/basic-routing.js",
    "bench:debug": "cross-env DEBUG=multimethods node ./dist/extras/bench/basic-routing.js",
    "prepare": "npm run self-ref && npm run build",
    "self-ref": "node ./scripts/enable-self-reference"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/yortus/multimethods.git"
  },
  "bugs": {
    "url": "http://github.com/yortus/multimethods/issues"
  },
  "keywords": [
    "multimethod",
    "multi method",
    "polymorphic",
    "dynamic",
    "dispatch",
    "function",
    "method",
    "override"
  ],
  "contributors": [
    {
      "name": "Troy Gerwien",
      "email": "yortus@gmail.com",
      "url": "http://github.com/yortus/"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/yortus/multimethods#readme",
  "dependencies": {
    "debug": "^2.6.8"
  },
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/debug": "0.0.29",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.0",
    "chai": "^4.0.2",
    "cross-env": "^5.0.1",
    "mocha": "^3.4.2",
    "pegjs": "^0.10.0",
    "rimraf": "^2.6.1",
    "typescript": "^2.4.1"
  }
}
